$ mkdir q2
$ cd q2
$ git init
$ echo iitbhilai >>11940160.txt    \\explicitly this command will create .txt file
$ cp 11940160.txt 11940020.txt
$ cp 11940160.txt 11941040.txt
$ mkdir AnupamKumar
$ mkdir AbdurRahmanKhan
$ mkdir RuchitPrakashSaxena
$ mv 11940160.txt AnupamKumar
$ mv 11940020.txt AbdurRahmanKhan
$ mv 11941040.txt RuchitPrakashSaxena
$ git add AnupamKumar
$ git commit -m ”commited 1st member ”              \\commiting changes to files one by one
$ git graph                       \\this will make git-graph pdf first time after committing details of 1st member
$ git add AbdurRahmanKhan
$ git commit -m ”commited 2nd member ”
$ git graph 
$ git add RuchitPrakashSaxena
$ git commit -m ”commited 3rd member ”
$ git graph
